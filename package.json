{
  "name": "smart-agents",
  "version": "0.1.0",
  "description": "Intelligent AI Agent Ecosystem optimized for MacBook Pro M2",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "test": "vitest",
    "test:coverage": "vitest --coverage",
    "test:e2e": "echo '⚠️  WARNING: Use test:e2e:safe instead to prevent system freeze!' && exit 1",
    "test:e2e:collaboration": "echo '⚠️  WARNING: Use test:e2e:collaboration:safe instead!' && exit 1",
    "test:e2e:security": "echo '⚠️  WARNING: Use test:e2e:security:safe instead!' && exit 1",
    "test:e2e:safe": "./scripts/e2e-resource-monitor.sh npm run test:e2e",
    "test:e2e:collaboration:safe": "./scripts/e2e-resource-monitor.sh npm run test:e2e:collaboration",
    "test:e2e:security:safe": "./scripts/e2e-resource-monitor.sh npm run test:e2e:security",
    "lint": "eslint src --ext .ts",
    "format": "prettier --write 'src/**/*.ts'",
    "typecheck": "tsc --noEmit",
    "orchestrator": "tsx src/orchestrator/index.ts",
    "rag": "tsx src/agents/rag/index.ts",
    "rag:demo": "tsx src/agents/rag/demo.ts",
    "rag:index": "echo 'RAG indexing not yet implemented'",
    "dashboard": "tsx src/dashboard/server.ts",
    "demo:architecture": "tsx src/agents/architecture/demo.ts"
  },
  "keywords": [
    "ai",
    "agents",
    "claude",
    "openai",
    "orchestrator",
    "rag",
    "mcp"
  ],
  "author": "Your Name",
  "license": "MIT",
  "dependencies": {
    "@anthropic-ai/sdk": "^0.30.1",
    "@types/better-sqlite3": "^7.6.13",
    "@types/form-data": "^2.2.1",
    "@types/multer": "^2.0.0",
    "axios": "^1.13.2",
    "better-sqlite3": "^12.5.0",
    "chromadb": "^1.9.2",
    "cors": "^2.8.5",
    "dotenv": "^16.4.7",
    "express": "^5.2.1",
    "form-data": "^4.0.5",
    "multer": "^2.0.2",
    "openai": "^4.70.4",
    "sqlite": "^5.1.1",
    "sqlite3": "^5.1.7",
    "uuid": "^13.0.0",
    "vectra": "^0.11.1",
    "zod": "^3.24.1"
  },
  "devDependencies": {
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.6",
    "@types/node": "^22.10.2",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^8.18.2",
    "@typescript-eslint/parser": "^8.18.2",
    "@vitest/coverage-v8": "^2.1.8",
    "eslint": "^9.17.0",
    "prettier": "^3.4.2",
    "tsx": "^4.19.2",
    "typescript": "^5.7.2",
    "vitest": "^2.1.8"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  }
}
